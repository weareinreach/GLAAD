		"dev": "next dev",
		"dev:ui": "storybook dev -p 6006",
		"format": "prettier --write --ignore-unknown .",
		"preinstall": "npx only-allow pnpm",
		"postinstall": "prisma generate",
		"lint": "next lint",
		"lint:fix": "next lint --fix",
		"lint:staged": "lint-staged",
		"prepare": "tsx ./.lib/prepare.ts",
		"start": "next start"
	},
