	"private": true,
	"scripts": {
		"build": "turbo build:next",
		"build-storybook": "storybook build",
		"build:next": "next build",
		"chromatic": "chromatic --exit-zero-on-changes",
		"db:deploy": "prisma migrate deploy",
		"db:down": "cd docker && docker compose down",
